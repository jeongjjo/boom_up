<% for (i in data) {
    var rank = (sort == '24rank' || sort == 'allrank') || (kind == "search") ? (page*1)+(i*1)+1 : 0
%>
    <% if (data[i].listType == "post") { %>
        <%- include('../board/item', {data: data[i], idx: i, rank: rank}) -%>
    <% } else { %>
        <%- include('../comment/item', {data: data[i], isProfile : "myprofile"}) -%>
    <% } %>
<% } %>